<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0"
         metadata-complete="false">

    <display-name>Archetype Created Web Application</display-name>

    <context-param>
        <param-name>username</param-name>
        <param-value>张三</param-value>
    </context-param>

    <servlet>
        <display-name>HelloServlet</display-name>
        <servlet-name>HelloServlet</servlet-name>
        <servlet-class>org.zz.servlet.HelloServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HelloServlet</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>

    <servlet>
        <display-name>ExampleServlet1</display-name>
        <servlet-name>ExampleServlet1</servlet-name>
        <servlet-class>org.zz.servlet.ExampleServlet1</servlet-class>
        <init-param>
            <param-name>name</param-name>
            <param-value>仔仔</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>ExampleServlet1</servlet-name>
        <url-pattern>/example1</url-pattern>
    </servlet-mapping>

    <servlet>
        <display-name>ExampleServlet2</display-name>
        <servlet-name>ExampleServlet2</servlet-name>
        <servlet-class>org.zz.servlet.ExampleServlet2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ExampleServlet2</servlet-name>
        <url-pattern>/example2</url-pattern>
    </servlet-mapping>
</web-app>
